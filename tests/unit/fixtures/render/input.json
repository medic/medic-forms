[
  {
    "_name": "Unknown renderer",

    "form": {
      "meta": {
        "id": "form"
      },
      "fields": [{
        "id": "moniker",
        "name": "Patient Name",
        "type": "hoozit"
      }]
    },
    "expect": {
      "valid": false,
      "error": "No renderers could be found for 'moniker'."
    }
  },

  {
    "_name": "Unknown renderer in second field",

    "form": {
      "meta": {
        "id": "form"
      },
      "fields": [
        {
          "id": "moniker",
          "name": "Patient Name",
          "type": "string"
        },
        {
          "id": "hoozit",
          "name": "Huh?",
          "type": "hoozit"
        }
      ]
    },
    "expect": {
      "valid": false,
      "error": "No renderers could be found for 'hoozit'."
    }
  },

  {
    "_name": "Unknown named renderer",

    "form": {
      "meta": {
        "id": "form"
      },
      "fields": [{
        "id": "moniker",
        "name": "Patient Name",
        "type": "string",
        "render": "hoozit"
      }]
    },
    "expect": {
      "valid": false,
      "error": "No renderers could be found for 'moniker'."
    }
  },

  {
    "_name": "Single string field",

    "form": {
      "meta": {
        "id": "form"
      },
      "fields": [{
        "id": "moniker",
        "name": "Patient Name",
        "type": "string"
      }]
    },
    "expect": {
      "valid": true,
      "result": "<form><field><id>moniker</id></field></form>"
    }
  },

  {
    "_name": "Single string field with validation error",

    "form": {
      "meta": {
        "id": "form"
      },
      "fields": [{
        "id": "moniker",
        "name": "Patient Name",
        "type": "string"
      }]
    },
    "values": {
      "moniker": ""
    },
    "validation": {
      "valid": false,
      "detail": {
        "moniker": {
          "valid": false,
          "error": "wrong"
        }
      }
    },
    "expect": {
      "valid": true,
      "result": "<form><field><error>wrong</error><id>moniker</id></field></form>"
    }
  },

  {
    "_name": "Single string field with validation error on initial render",

    "form": {
      "meta": {
        "id": "form"
      },
      "fields": [{
        "id": "moniker",
        "name": "Patient Name",
        "type": "string"
      }]
    },
    "values": {
      "moniker": ""
    },
    "validation": {
      "valid": false,
      "detail": {
        "moniker": {
          "valid": false,
          "error": "wrong"
        }
      }
    },
    "options": {
      "initial": true
    },
    "expect": {
      "valid": true,
      "result": "<form><field><id>moniker</id></field></form>"
    }
  },

  {
    "_name": "Single string field with value",

    "form": {
      "meta": {
        "id": "form"
      },
      "fields": [{
        "id": "moniker",
        "name": "Patient Name",
        "type": "string"
      }]
    },
    "values": {
      "moniker": "joe"
    },
    "validation": {
      "valid": true,
      "detail": {
        "moniker": {
          "valid": true
        }
      }
    },
    "expect": {
      "valid": true,
      "result": "<form><field><id>moniker</id><value>joe</value></field></form>"
    }
  },

  {
    "_name": "Single string field with default",

    "form": {
      "meta": {
        "id": "form"
      },
      "fields": [{
        "id": "moniker",
        "name": "Patient Name",
        "type": "string",
        "default": "jack"
      }]
    },
    "expect": {
      "valid": true,
      "result": "<form><field><id>moniker</id><value>jack</value></field></form>"
    }
  },

  {
    "_name": "Multiple string fields",

    "form": {
      "meta": {
        "id": "form"
      },
      "fields": [
        {
          "id": "moniker",
          "name": "Patient Name",
          "type": "string"
        },
        {
          "id": "prefix",
          "name": "Title",
          "type": "string"
        }
      ]
    },
    "expect": {
      "valid": true,
      "result": "<form><field><id>moniker</id></field><field><id>prefix</id></field></form>"
    }
  },

  {
    "_name": "Single textarea field",

    "form": {
      "meta": {
        "id": "form"
      },
      "fields": [{
        "id": "description",
        "name": "Description",
        "type": "string",
        "render": "textarea"
      }]
    },
    "expect": {
      "valid": true,
      "result": "<form><textarea><id>description</id></textarea></form>"
    }
  },

  {
    "_name": "Required field (simple)",

    "form": {
      "meta": {
        "id": "form"
      },
      "fields": [{
        "id": "description",
        "name": "Description",
        "type": "string",
        "required": true
      }]
    },
    "expect": {
      "valid": true,
      "result": "<form><field><id>description</id><required/></field></form>"
    }
  },

  {
    "_name": "Required field (javascript true)",

    "form": {
      "meta": {
        "id": "form"
      },
      "fields": [{
        "id": "description",
        "name": "Description",
        "type": "string",
        "required": {
          "javascript": "return 1 === 1"
        }
      }]
    },
    "expect": {
      "valid": true,
      "result": "<form><field><id>description</id><required/></field></form>"
    }
  },

  {
    "_name": "Required field (javascript false)",

    "form": {
      "meta": {
        "id": "form"
      },
      "fields": [{
        "id": "description",
        "name": "Description",
        "type": "string",
        "required": {
          "javascript": "return 1 !== 1"
        }
      }]
    },
    "expect": {
      "valid": true,
      "result": "<form><field><id>description</id></field></form>"
    }
  },

  {
    "_name": "Repeating field",

    "form": {
      "meta": {
        "id": "form"
      },
      "fields": [{
        "id": "description",
        "name": "Description",
        "type": "string",
        "repeat": true
      }]
    },
    "expect": {
      "valid": true,
      "result": "<form><repeat><field><id>description</id></field></repeat></form>"
    }
  },

  {
    "_name": "Repeating field defaults",

    "form": {
      "meta": {
        "id": "form"
      },
      "fields": [{
        "id": "description",
        "name": "Description",
        "type": "string",
        "repeat": true,
        "default": ["a", "b"]
      }]
    },
    "expect": {
      "valid": true,
      "result": "<form><repeat><field><id>description</id></field><field><id>description[0]</id><value>a</value></field><field><id>description[1]</id><value>b</value></field></repeat></form>"
    }
  },

  {
    "_name": "Repeating field values",

    "form": {
      "meta": {
        "id": "form"
      },
      "fields": [{
        "id": "description",
        "name": "Description",
        "type": "string",
        "repeat": true
      }]
    },
    "values": {
      "description": ["a", "b"]
    },
    "expect": {
      "valid": true,
      "result": "<form><repeat><field><id>description</id></field><field><id>description[0]</id><value>a</value></field><field><id>description[1]</id><value>b</value></field></repeat></form>"
    }
  },

  {
    "_name": "Nested field values",

    "form": {
      "meta": {
        "id": "form"
      },
      "fields": [{
        "id": "address",
        "name": "Address",
        "type": "fields",
        "fields": [{
          "id": "street",
          "name": "Street",
          "type": "string"
        }]
      }]
    },
    "expect": {
      "valid": true,
      "result": "<form><nest><id>address</id><field><id>address.street</id></field></nest></form>"
    }
  }
]