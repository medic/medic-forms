[{
  "meta": {
    "id": "test",
    "name": "Example Form",
    "description": "This is an example form"
  },
  "fields": [
    {
      "id": "q1",
      "type": "select",
      "name": "Greeting",
      "description": "Are you arriving or departing?",
      "items": [
        [ "HI", { "en": "Hello", "fr": "Bonjour" }],
        [ "BYE", "Goodbye" ]
      ]
    },
    {
      "id": "q1a",
      "type": "select",
      "items": [
        [ 1, "Now" ],
        [ 2, "Later" ]
      ],
      "conditions": {
        "$structured": { "q1": "BYE" }
      }
    },
    {
      "id": "q2",
      "type": "string",
      "length": [ 3, 45 ],
      "conditions": {
        "$structured": [
          { "q1a": 2 },
          { "$operator": "&&" },
          { "q1": "BYE" }
        ]
      },
      "validations": {
        "$builtin": "is_phone_number"
      }
    }
  ],
  "_name": "Simple validations (structured, builtin)"
}],

